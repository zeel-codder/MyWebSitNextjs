{"ast":null,"code":"/**\n This page all information about zeel-codder education info.\n\n @name:Projects\n @type:React Component\n @param:none\n @returns: JSX of Education details\n @functionality : This Component is use to show Project info of zeel in website.\n**/\n// Imports\n// ====================================\n// import {IoIosArrowDroprightCircle} from 'react-icons/io'\n// import {FaRegHandPointRight} from 'react-icons/fa'\n// import Brightness1Icon from '@material-ui/icons/Brightness1';\n// import Brightness5Icon from '@material-ui/icons/Brightness5';\nimport ArrowRightIcon from '@material-ui/icons/ArrowRight';\nimport BookmarkIcon from '@material-ui/icons/Bookmark';\nimport { useState } from 'react';\nimport React from \"react\"; // import { useGlobalContext } from '../context';\n// ====================================\n\nvar Projects = () => {\n  var ProjectData = [{\n    id: 1,\n    title: 'My Introduction',\n    technology: 'React js',\n    subtitle: 'This projects is build on react js.',\n    codelink: 'https://github.com/zeel-codder/My_Introduction',\n    link: 'https://zeelcodder.tech/'\n  }, {\n    id: 2,\n    title: 'BookMarkStor',\n    technology: 'Html,Css and Js',\n    subtitle: 'This is used for Storing bookmark of website.',\n    codelink: 'https://github.com/zeel-codder/BookMarks',\n    link: 'https://mybookmarks1.netlify.app/'\n  }]; //Store the open ul data\n\n  var [arr, setarr] = useState([]); //=============================\n  // @name:changeIndex\n  // @type:Javascript Function\n  // @param:none\n  // @return:none\n  // @functionality: Function is used to show the Text of ul. It item in arr than it nested ul is display in screen other wise not.\n  //=============================\n\n  var changeIndex = index => {\n    // console.log(index)\n    var new_arr = arr;\n\n    if (new_arr.includes(index)) {\n      new_arr = new_arr.filter(index1 => index1 !== index);\n    } else {\n      new_arr.push(index);\n    } // console.log([...new_arr])\n\n\n    setarr([...new_arr]);\n  }; // console.log('rendee')\n\n\n  return /*#__PURE__*/React.createElement(\"div\", {\n    id: \"projects\"\n  }, /*#__PURE__*/React.createElement(\"h1\", null, \"Projects\"), /*#__PURE__*/React.createElement(\"div\", {\n    className: \"box row\",\n    id: \"projects\"\n  }, /*#__PURE__*/React.createElement(\"div\", {\n    className: \"list-container\"\n  }, /*#__PURE__*/React.createElement(\"ul\", {\n    className: \"project-ul\"\n  }, ProjectData.map((d, index) => {\n    var class1 = \"not-show\";\n    var class2 = \"\";\n\n    if (arr.includes(index)) {\n      class1 = \"show\";\n      class2 = \"rotetor\";\n    }\n\n    return /*#__PURE__*/React.createElement(\"li\", {\n      key: d.id,\n      onClick: () => changeIndex(index)\n    }, /*#__PURE__*/React.createElement(\"p\", {\n      className: \"text\"\n    }, /*#__PURE__*/React.createElement(ArrowRightIcon, {\n      className: class2\n    }), \"  \\xA0\", d.title, \" , \", d.technology), /*#__PURE__*/React.createElement(\"ul\", {\n      className: class1\n    }, /*#__PURE__*/React.createElement(\"li\", null, /*#__PURE__*/React.createElement(BookmarkIcon, null), \" \", d.subtitle), /*#__PURE__*/React.createElement(\"li\", null, /*#__PURE__*/React.createElement(BookmarkIcon, null), \" Code Link: \", /*#__PURE__*/React.createElement(\"a\", {\n      href: d.codelink,\n      style: {\n        padding: 1,\n        fontSize: '14px'\n      }\n    }, \"Code\")), /*#__PURE__*/React.createElement(\"li\", null, /*#__PURE__*/React.createElement(BookmarkIcon, null), \" Project Link: \", /*#__PURE__*/React.createElement(\"a\", {\n      href: d.link,\n      style: {\n        padding: 1,\n        fontSize: '14px'\n      }\n    }, \"Link\"))));\n  })))));\n};\n\nexport default Projects;","map":null,"metadata":{},"sourceType":"module"}